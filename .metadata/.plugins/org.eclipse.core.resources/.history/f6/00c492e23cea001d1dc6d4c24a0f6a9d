
public class sub4 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a=3;
		int b=0;
		a+=3;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		b++;
		System.out.println(a*b);
		
		int x=0;
		 System.out.println(x++); //출력은 0이지만 x에는 1이 저장됨
		 System.out.println(x);
	}

}
