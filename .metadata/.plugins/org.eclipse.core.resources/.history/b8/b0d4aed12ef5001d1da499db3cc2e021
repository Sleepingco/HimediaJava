
public class Phone {//부모클래스,SuperClass,ParentClass
	String model;	//부모클래스가 Private이면 상속이 안된다.
	String color;
	
	public String getModel() {
		return model;
	}
	public void setModel(String model) {
		this.model = model;
	}
	public String getColor() {
		return color;
	}
	public void setColor(String color) {
		this.color = color;
	}
}
